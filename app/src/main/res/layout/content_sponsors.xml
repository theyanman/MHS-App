<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.codeforcharity.michaelyanyoga.mhsappredesign.SponsorsActivity"
    tools:showIn="@layout/activity_sponsors">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="A Special Thanks to Our Sponsors!"
        android:id="@+id/textView16"
        android:textStyle="bold"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="26sp"
        android:gravity="center_horizontal"
        android:textIsSelectable="true" />

    <ImageView

        android:id="@+id/imageView2"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:scaleType="fitXY"
        android:src="@drawable/c4clogo"
        android:layout_below="@+id/textView16"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="71dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Code 4 Charity Club"
        android:id="@+id/textView17"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
